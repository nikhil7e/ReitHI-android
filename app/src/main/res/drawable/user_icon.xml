<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <size
                android:width="10dp"
                android:height="10dp" />
            <stroke
                android:width="0.5dp"
                android:color="#FFFFFF" />

        </shape>
    </item>

    <item android:gravity="center" android:top="-1.9dp">
        <shape android:shape="oval">
            <size
                android:width="4dp"
                android:height="4dp" />
            <stroke
                android:width="0.5dp"
                android:color="#FFFFFF" />
        </shape>
    </item>

    <item android:gravity="center" android:bottom="-5.9dp">
        <shape android:shape="oval">
            <size
                android:width="7dp"
                android:height="4dp" />
            <stroke
                android:width="0.5dp"
                android:color="#FFFFFF" />

        </shape>
    </item>



</layer-list>