<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="2dp" android:height="14dp" android:gravity="center">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="10dp"
                    android:height="10dp" />
                <solid android:color="?attr/colorOnPrimary" />
            </shape>
        </rotate>
    </item>
    <item android:width="2dp" android:height="14dp" android:gravity="center">
        <rotate
            android:fromDegrees="135"
            android:toDegrees="135">
            <shape android:shape="rectangle">
                <size
                    android:width="10dp"
                    android:height="10dp" />
                <solid android:color="?attr/colorOnPrimary" />
            </shape>
        </rotate>
    </item>
    <item android:width="20dp" android:height="20dp">
        <shape android:shape="oval">
            <size
                android:width="10dp"
                android:height="10dp" />
            <solid android:color="?attr/colorOnSecondary" />
        </shape>
    </item>
    <item android:width="2dp" android:height="14dp" android:gravity="center">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45">
            <shape android:shape="rectangle">
                <size
                    android:width="10dp"
                    android:height="10dp" />
                <solid android:color="?attr/colorOnPrimary" />
            </shape>
        </rotate>
    </item>
    <item android:width="2dp" android:height="14dp" android:gravity="center">
        <rotate
            android:fromDegrees="135"
            android:toDegrees="135">
            <shape android:shape="rectangle">
                <size
                    android:width="10dp"
                    android:height="10dp" />
                <solid android:color="?attr/colorOnPrimary" />
            </shape>
        </rotate>
    </item>

</layer-list>

